{
    "version": 2,
    "functions": {
        "src/main.ts": {
          "memory": 1024,
          "maxDuration": 30
        }
      },
    
    "routes": [
      {
        "src": "/(.*)",
        "dest": "src/main.ts",
        "methods": ["GET", "POST", "PUT", "PATCH", "HEAD", "DELETE", "OPTIONS"]
      }
    ]
}